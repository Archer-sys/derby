@import "nib/vendor";

p {
  margin: 0;
  padding: 0;
}
body {
  margin: 10px;
}
body,
select {
  font: 13px/normal arial,sans-serif;
}
table {
  border-collapse: collapse;
  border-spacing: 0;
}

.css {
  list-style: none;
  padding-left: 1em;
  margin: 0
}

#table-editor {
  user-select: none;
}
.editor-foot {
  text-align: center;
}
#table-editor th,
#drag-container th {
  cursor: move;
  padding: 0 4px;
}
#drag-container {
  display: none;
  position: absolute;
}